                      <a href="/wiki/Multitrack_recording" title="Multitrack recording">
                        multitrack audio
                      </a>
                       (
                      <a href="/wiki/Digital_audio_workstation" title="Digital audio workstation">
                        DAW
                      </a>
                      ) and 
                      <a href="/wiki/Audio_plug-in" title="Audio plug-in">
                        plug-ins
                      </a>
                       (
                      <a href="/wiki/Steinberg_Cubase" title="Steinberg Cubase">
                        Steinberg Cubase 6
                      </a>
                      <sup id="cite_ref-3" class="reference">
